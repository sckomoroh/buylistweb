<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>

    </ui:style>
    <g:VerticalPanel>
        <g:Cell width="100%">
            <g:HorizontalPanel width="100%" spacing="5">
                <g:Cell verticalAlignment="ALIGN_MIDDLE" width="77px">
                    <g:Label text="New name" width="77px" horizontalAlignment="ALIGN_LEFT" direction="LTR"/>
                </g:Cell>
                <g:Cell width="200">
                    <g:TextBox width="200px" ui:field="nameEditBox"/>
                </g:Cell>
            </g:HorizontalPanel>
        </g:Cell>
        <g:Cell width="100%" horizontalAlignment="ALIGN_RIGHT">
            <g:HorizontalPanel width="100%" spacing="5">
                <g:Cell width="" height="" horizontalAlignment="ALIGN_RIGHT">
                    <g:Button text="OK" ui:field="okButton"/>
                </g:Cell>
                <g:Cell width="62">
                    <g:Button text="Cancel" ui:field="cancelButton"/>
                </g:Cell>
            </g:HorizontalPanel>
        </g:Cell>
    </g:VerticalPanel>
</ui:UiBinder> 